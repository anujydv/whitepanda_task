{
    "presets": [
        [
            "@babel/env",
            {
                "targets": {
                    "node": "8",
                    "browsers": [
                        "last 2 versions"
                    ]
                },
                "useBuiltIns": "usage",
                "corejs": 3,
                "shippedProposals": true,
            }
        ],
        "@babel/react"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/syntax-dynamic-import",
        "react-hot-loader/babel"
    ],
    "sourceMaps": "inline"
}